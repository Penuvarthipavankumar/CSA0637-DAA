#include <stdio.h>

// Function to print the pattern
void printPattern(int n) {
    // Iterate over rows
    for (int i = 1; i <= n; i++) {
        int num = 1; // Initialize the number to print in each row
        // Iterate over columns
        for (int j = 1; j <= i; j++) {
            // Print the current number followed by a space
            printf("%d ", num);
            num++; // Increment the number for the next column
        }
        printf("\n"); // Move to the next line after each row
    }
}

int main() {
    int n = 4; // Define the value of n
    printPattern(n); // Call the function to print the pattern
    return 0;
}
